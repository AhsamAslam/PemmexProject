{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/organization",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "organization.api",
          "Port": "80"
        }
      ],
      "UpstreamPathTemplate": "/organization",
      "UpstreamHttpMethod": [ "Get", "Post" ],
      "RateLimitOptions": {
        "ClientWhitelist": [],
        "EnableRateLimiting": true,
        "Period": "5s",
        "PeriodTimespan": 1,
        "Limit": 1
      }
    },
    {
      "DownstreamPathTemplate": "/api/organization/employees",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "organization.api",
          "Port": "80"
        }
      ],
      "UpstreamPathTemplate": "/organization/employees",
      "UpstreamHttpMethod": [ "Get", "Post" ]
    },
    {
      "DownstreamPathTemplate": "/api/organization/employees/{id}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "organization.api",
          "Port": "80"
        }
      ],
      "UpstreamPathTemplate": "/organization/Employees/{id}",
      "UpstreamHttpMethod": [ "Get", "Put", "Delete" ]
    },
    {
      "DownstreamPathTemplate": "/api/organization/managers",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "organization.api",
          "Port": "80"
        }
      ],
      "UpstreamPathTemplate": "/organization/managers",
      "UpstreamHttpMethod": [ "Get", "Put", "Delete" ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:5010"
  }
}